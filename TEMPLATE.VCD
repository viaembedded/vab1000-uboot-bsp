     "Configure External LVDS By EDID"     BOOL
EndStruct

;============================================================================
;               Port characteristics Config(VCP_PortConfig)
;
BeginStruct	PORT_CHAR_CONFIG
    " Bit[3:0]: I2C port#(0xF means invalid), Bit[7:4]: reserved" BYTE 0
    " DAC_Char_Byte "            DWORD   1
	" LVDS_Char_Byte"            DWORD   1
	" DVO_Char_Byte "            DWORD   1
	" DP1_Char_Byte "            DWORD   1
	" DP2_Char_Byte "            DWORD   1
    " DSI_Char_Byte "            DWORD   1
	" Bit[3:0]: GPIO pin#(0xF means invalid), Bit[7:4]: reserved" BYTE 0
	" DAC_GPIO_BYTE "            DWORD   1
	" LVDS_GPIO_BYTE "           DWORD   1
	" DVO_GPIO_BYTE "            DWORD   1
	" DP1_GPIO_BYTE "            DWORD   1
	" DP2_GPIO_BYTE "            DWORD   1
    " DSI_GPIO_BYTE "            DWORD   1
EndStruct

;============================================================================
;		Boot CRTC Registers (VCP_CR_DEFAULT_TABLE)
;
BeginStruct	    Boot_CR
EndStruct


;============================================================================
