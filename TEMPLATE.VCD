     "Dual Channel"                   BOOL        1
     "24 Bit Pannel"                  BOOL        1
     "Enable Dither"                  BOOL        1
     "MSB Color Mapping"              BOOL        1
     "SKIP"                                       7
     "Min Off to On Wait time (ms)"   DEC DWORD   1
     "Default SSC range"              DEC DWORD   1
     "Panel EDID detailed timing"     BYTE        0
     "Pixel clock / 10,000"           DEC DWORD   1
     "SKIP"                                       4
     "Horizontal Active"              DWORD       1
     "Horizontal Blanking"            DWORD       1
     "Horizontal Active/Blanking"     DWORD       1
     "Vertical Active"                DWORD       1
     "Vertical Blanking"              DWORD       1
     "Vertical Active/Blanking"       DWORD       1
     "Horizontal Sync Offset"         DWORD       1
     "Horizontal Sync Pulse Width"    DWORD       1
     "Vertical Sync Offset/Width"     DWORD       1
     "H/V Sync Offset/width"          DWORD       1
     "Horizontal Image Size"          DWORD       1
     "Vertical Image Size"            DWORD       1
     "H/V Image Size"                 DWORD       1
     "Horizontal Border"              DWORD       1
     "Vertical Border"                DWORD       1
     "Flags"                          DWORD       1
     "Panel Power Sequence (ms)"      BYTE        0
     "VDD High To Data Active"        DEC DWORD    1
     "Data Active To VEE High"        DEC DWORD    1
     "VEE Low To Data Inactive"       DEC DWORD    1
     "Data Inactive To VDD Low"       DEC DWORD    1
EndTable

