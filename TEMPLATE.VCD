     "CRT1"          BOOL
     "Internal LCD"  BOOL
     "TV"            BOOL
     "HDTV"          BOOL
     "Reserved"      BOOL
     "DVO"           BOOL
     "Reserved"      BOOL
     "Reserved"      BOOL
     "Reserved"      BOOL
     "DP6"           BOOL
     "Reserved"      BOOL
     "Reserved"      BOOL
     "Reserved"      BOOL
     "Reserved"      BOOL
     "Reserved"      BOOL
     "DP5"           BOOL
EndStruct

;============================================================================
;		Supported DVO Devices (VCP_SupportedDVODeviceType)
;
BeginStruct     Supported_DVO_Device
     "AD9389B"        CONST BOOL
     "VT1632A"        CONST BOOL
     "VT1636"         CONST BOOL
     "VT1625"         CONST BOOL
     "CH7301C"        CONST BOOL
     "CH7305"         CONST BOOL
     "ADV7511"        CONST BOOL
EndStruct

;============================================================================
;		Boot Device Priority (VCP_BootDevPriority)
;
BeginStruct     BootDev_Opt
     "Priority 1  ActDisp"             DWORD
     "Priority 2  ActDisp"             DWORD
     "Priority 3  ActDisp"             DWORD
     "Priority 4  ActDisp"             DWORD
     "Priority 5  ActDisp"             DWORD
     "Priority 6  ActDisp"             DWORD
     "Priority 7  ActDisp"             DWORD
     "Priority 8  ActDisp"             DWORD
     "Priority 9  ActDisp"             DWORD
     "Priority 10 ActDisp"             DWORD
